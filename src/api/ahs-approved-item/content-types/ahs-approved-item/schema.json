{
  "kind": "collectionType",
  "collectionName": "ahs_approved_items",
  "info": {
    "singularName": "ahs-approved-item",
    "pluralName": "ahs-approved-items",
    "displayName": "ahsApprovedItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "location": {
      "type": "string"
    },
    "gMaps": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "biginteger"
    },
    "instagram": {
      "type": "string"
    },
    "restaurantImage": {
      "type": "component",
      "component": "ahs-approved.restaurant-image",
      "repeatable": true
    },
    "subTitle": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "maxLength": 300
    }
  }
}
