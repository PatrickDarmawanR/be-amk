{
  "kind": "collectionType",
  "collectionName": "article_items",
  "info": {
    "singularName": "article-item",
    "pluralName": "article-items",
    "displayName": "articleItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "publishedDateTime": {
      "type": "datetime"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "richtext"
    }
  }
}
